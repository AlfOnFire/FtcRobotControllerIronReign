<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice
        name="Control Hub Portal"
        parentModuleAddress="173"
        serialNumber="(embedded)">
        <LynxModule
            name="Control Hub"
            port="173">
            <RevRoboticsUltraplanetaryHDHexMotor
                name="go1"
                port="0" />
            <ContinuousRotationServo
                name="yaw1"
                port="0" />

            <RevRoboticsUltraplanetaryHDHexMotor
                name="go2"
                port="1" />
            <ContinuousRotationServo
                name="yaw2"
                port="1" />

            <RevRoboticsUltraplanetaryHDHexMotor
                name="go3"
                port="2" />
            <ContinuousRotationServo
                name="yaw3"
                port="2" />

            <LynxEmbeddedIMU
                name="imu"
                bus="0"
                port="0" />
        </LynxModule>

        <LynxModule
            name="Expansion Hub 2"
            port="2">
            <RevRoboticsUltraplanetaryHDHexMotor
                name="encoder3"
                port="2" />
            <RevRoboticsUltraplanetaryHDHexMotor
                name="encoder2"
                port="1" />
            <RevRoboticsUltraplanetaryHDHexMotor
                name="encoder1"
                port="0" />
            <LynxEmbeddedIMU
                name="eimu"
                bus="0"
                port="0" />
        </LynxModule>
    </LynxUsbDevice>
</Robot>
